{
    "name": "pollen-solutions/installer",
    "description": "Pollen Solution application installer.",
    "keywords": [
        "pollen-solutions",
        "installer"
    ],
    "license": "MIT",
    "time": "2022-12-30",
    "authors": [
        {
            "name": "Jordy Manner",
            "email": "jordy@presstify.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "symfony/console": "^4.0|^5.0|^6.0",
        "symfony/process": "^4.2|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.3",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Pollen\\Installer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pollen\\Installer\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "bin": [
        "bin/pollen"
    ]
}
